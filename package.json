{
    "name": "ngu-carousel-example",
    "version": "4.0.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uiuniversal/ngu-carousel.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "format:check": "prettier --config ./.prettierrc --list-different \"{projects,src}/**/*{.ts,.js,.json,.html}\"",
        "format:all": "prettier --config ./.prettierrc --write \"{projects,src}/**/*{.ts,.js,.json,.html}\"",
        "build:lib": "ng build @ngu/carousel",
        "build:app": "ng build",
        "deploy:app": "ng build @ngu/carousel && ng build",
        "run-lib": "npm run build:lib && ng serve -o",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.2.0",
        "@angular/cdk": "^12.2.13",
        "@angular/common": "~12.2.0",
        "@angular/compiler": "~12.2.0",
        "@angular/core": "~12.2.0",
        "@angular/forms": "~12.2.0",
        "@angular/material": "^12.2.13",
        "@angular/platform-browser": "~12.2.0",
        "@angular/platform-browser-dynamic": "~12.2.0",
        "@angular/router": "~12.2.0",
        "hammerjs": "^2.0.8",
        "rxjs": "~6.6.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.13",
        "@angular/cli": "~12.2.13",
        "@angular/compiler-cli": "~12.2.0",
        "@angular/language-service": "^13.0.1",
        "@types/hammerjs": "^2.0.40",
        "@types/jasmine": "~3.8.0",
        "@types/jasminewd2": "^2.0.10",
        "@types/node": "^12.11.1",
        "jasmine-core": "~3.8.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ng-packagr": "^12.1.1",
        "prettier": "^2.4.1",
        "protractor": "^7.0.0",
        "ts-node": "^10.4.0",
        "typescript": "~4.3.5"
    },
    "keywords": [
        "angular",
        "carousel",
        "universal",
        "angular universal",
        "universal carousel",
        "ngu carousel"
    ]
}
